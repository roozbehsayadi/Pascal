var
  a:integer;
begin
  writeln('enter a number');
  readln(a);

  if a=1 then
    writeln('you will have a great success tomorrow')
  else if a=2 then
    writeln('youll have an accident tomorrow')
  else if a=3 then
    writeln('good news')
  else if a=4 then
    writeln('NULL!!!')
  else if a=5 then
    writeln('be hopeful')
  else if a=6 then
    writeln('youll win a prize')
  else if a=7 then
    writeln('youll die tomorrow!')
  else if a=8 then
    writeln('youll reach your goal')
  else if a=9 then
    writeln('youll lose a prize')
  else
    writeln('you failed!');
  readln;
end.
